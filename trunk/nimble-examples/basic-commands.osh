install = { osgi:installBundle $it };
start = { (bundle $it) osgi:start };
update = { (bundle $it) osgi:update };
stop = { (bundle $it) osgi:stop };
uninstall = { (bundle $it) osgi:uninstall };
services = { osgi:allServiceReferences null null };
installAndStart = { 
  id = install $it;
  $id start
};
